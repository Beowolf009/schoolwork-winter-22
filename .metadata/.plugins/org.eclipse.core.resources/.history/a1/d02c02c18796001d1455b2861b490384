
public class listWords {

	public static void main(String[] args) {
	    String sentence =  "The quick brown fox jumps over the lazy dog."; 
        String[] word = sentence.split("\\s+"); 
        int wordCount = 0; 
        int current = 0;
        
        
        for(String w : word)
            System.out.println(w);
        
        

	}

}
